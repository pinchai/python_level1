class Class:
    __name = None
    __no = None
    __year = None
    __student = None

    def __init__(self, name, no, year, student):
        self.__name = name
        self.__no = no
        self.__year = year
        self.__student = student

    def getName(self):
        return self.__name

    def getNo(self):
        return self.__no

    def getYear(self):
        return self.__year

    def getStudent(self):
        return self.__student

    def __str__(self):
        return f"Group: {self.__name} {self.__no}\nYear: {self.__year}\nStudent: {self.__student}"

    def __add__(self, other):
        no = f"{self.__no}{other.getNo()}"
        student = self.__student + other.getStudent()
        return f"Group: {self.__name}{no}\n" \
               f"Year: {self.__year}\n" \
               f"Student: {student}"
